{
    "version": "2.0.0",
    "tasks": [
		{
			"label": "Make Directories",
			"type": "process",
			"command": "${workspaceFolder}\\make_dir.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		},
		{
			"label": "StrafeNet Create Deployment",
			"type": "process",
			"command": "${workspaceFolder}\\make_deployment.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		},
		{
			"label": "StrafeNet Copy Deployment to FPS Project",
			"type": "process",
			"command": "${workspaceFolder}\\copy_to_fps_project.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		},
		{
			"label": "StrafeNet Debug Build",
			"type": "process",
			"command": "${workspaceFolder}\\build_debug.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"dependsOn": [
				"Make Directories"
			],
			"group": "build",
			"problemMatcher": "$msCompile"
		},
		{
			"label": "Create and copy Deployment to FPS Project",
			"dependsOn": [
				"StrafeNet Create Deployment",
				"StrafeNet Copy Deployment to FPS Project"
			],
			"group": "none",
			"problemMatcher": []
		},
		{
			"label": "Strafenet Release Build",
			"type": "process",
			"command": "${workspaceFolder}\\build_release.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"dependsOn": [
				"Make Directories"
			],
			"group": "build"
		}
	]
}