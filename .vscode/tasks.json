{
    "version": "2.0.0",
    "tasks": [
		{
			"label": "Make Directories",
			"type": "process",
			"command": "${workspaceFolder}\\make_dir.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		},
		{
			"label": "Create Lib/Include Folder",
			"type": "process",
			"command": "${workspaceFolder}\\make_deployment.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		},
		{
			"label": "Copy Lib/Include to FPS Project",
			"type": "process",
			"command": "${workspaceFolder}\\copy_to_fps_project.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "none"
		},
		{
			"label": "Debug Batch Build",
			"type": "process",
			"command": "${workspaceFolder}\\build_debug.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"dependsOn": [
				"Make Directories"
			],
			"group": "build",
			"problemMatcher": "$msCompile"
		},
		{
			"label": "Create and copy Lib/Include to FPS Project",
			"dependsOn": [
				"Create Lib/Include Folder",
				"Copy Lib/Include to FPS Project"
			],
			"group": "none",
			"problemMatcher": []
		},
		{
			"label": "Release Batch Build",
			"type": "process",
			"command": "${workspaceFolder}\\build_release.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"dependsOn": [
				"Make Directories"
			],
			"group": "build"
		}
	]
}